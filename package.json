{
  "name": "auth-example-block",
  "version": "0.1.0",
  "description": "Auth dummy microservice for KintoHub",
  "main": "app/index.js",
  "scripts": {
    "dev": "babel-node app/index.js",
    "start": "nodemon --exec npm run dev",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:server": "babel -d ./dist ./app",
    "prod:build": "npm run build:clean && npm run build:server",
    "prod:server": "PORT=80 node dist/index.js",
    "prod": "npm run prod:build && npm run prod:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kintohub/auth-example-block.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kintohub/auth-example-block/issues"
  },
  "homepage": "https://github.com/kintohub/auth-example-block#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.21.5",
    "nodemon": "^1.17.4"
  },
  "dependencies": {
    "hapi": "^17.4.0"
  }
}
